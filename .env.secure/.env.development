# Development Environment Variables
NODE_ENV=development

# JWT Configuration
JWT_SECRET=dev-jwt-secret-change-in-production-very-long-and-secure-key-12345
JWT_REFRESH_SECRET=dev-refresh-secret-change-in-production-very-long-and-secure-key-67890

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/q2_platform_dev
REDIS_URL=redis://localhost:6379

# Encryption Keys
ENCRYPTION_KEY=dev-encryption-key-32-chars-long-change-in-production-12345678
LOG_SIGNING_KEY=dev-log-signing-key-change-in-production-secure-key-123456

# API Keys (Development)
EXCHANGE_API_KEY_BINANCE=dev-binance-api-key
EXCHANGE_SECRET_BINANCE=dev-binance-secret-key

# Security Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
MAX_LOGIN_ATTEMPTS=5

# Monitoring
LOG_LEVEL=debug
ENABLE_AUDIT_LOGGING=true

# External Services
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USER=dev
SMTP_PASS=dev

# Biometric Authentication
WEBAUTHN_ORIGIN=http://localhost:3000
WEBAUTHN_RP_ID=localhost
WEBAUTHN_RP_NAME=Q2 Platform Development

# Compliance
GDPR_ENABLED=true
PCI_DSS_ENABLED=false
FINANCIAL_COMPLIANCE_ENABLED=false
